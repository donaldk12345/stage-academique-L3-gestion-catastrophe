<form method="post" action="{{ path('app_pays_delete', {'id': p.id}) }}" onsubmit="return confirm('Voulez vous supprimer ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ p.id) }}">
    <button class="btn btn-sm bg-danger-light mt-2" style="margin-top:5px;"><i class="fas fa-trash"></i></button>
</form>
